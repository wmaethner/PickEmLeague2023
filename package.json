{
  "name": "pickemleague2023_mobile",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "ts:check": "tsc",
    "api": "npm run pregenerate && npm run convert && npm run clear-api && npm run generate && npm run replace",
    "pregenerate": "curl http://pickemleague2023-env.eba-8mjsx79p.us-east-1.elasticbeanstalk.com/api/swagger.json > openapi_2_0.json",
    "convert": "curl -X POST https://converter.swagger.io/api/convert -H 'Content-Type: application/json' -d @openapi_2_0.json > openapi.json",
    "clear-api": "rimraf ./src/apis",
    "generate": "openapi-generator-cli generate -i openapi.json -o ./src/apis --generator-name typescript-fetch",
    "replace": "replace-in-file --configFile=./replace.json --isRegex",
    "build": "eas build --platform all",
    "build-ios": "eas build --platform ios",
    "build-android": "eas build --platform android",
    "submit-ios": "eas submit -p ios",
    "submit-android": "eas submit -p android"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.18.2",
    "@react-navigation/bottom-tabs": "^6.5.8",
    "@react-navigation/native": "^6.1.7",
    "@types/react": "~18.2.14",
    "expo": "~49.0.7",
    "expo-auth-session": "~5.0.2",
    "expo-constants": "~14.4.2",
    "expo-crypto": "~12.4.1",
    "expo-linking": "~5.0.2",
    "expo-localization": "~14.3.0",
    "expo-router": "2.0.0",
    "expo-status-bar": "~1.6.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "react": "18.2.0",
    "react-md": "^5.1.5",
    "react-moment": "^1.1.3",
    "react-native": "0.72.4",
    "react-native-device-info": "^10.9.0",
    "react-native-draggable-flatlist": "^4.0.1",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-localize": "^3.0.2",
    "react-native-reanimated": "~3.3.0",
    "react-native-reanimated-table": "^0.0.2",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-storage": "^1.0.1",
    "react-native-table-component": "^1.2.2",
    "replace-in-file": "^7.0.1",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.22.11",
    "sass": "^1.66.1"
  },
  "private": true
}
