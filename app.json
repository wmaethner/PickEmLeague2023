{
  "expo": {
    "name": "PickEmLeague2023_Mobile",
    "slug": "PickEmLeague2023_Mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "pickemleague2023-mobile",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "pickemleague2023",
      "supportsTablet": true,
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
          // "NSExceptionDomains": {
          //   "PickEmLeague2023-env.eba-8mjsx79p.us-east-1.elasticbeanstalk.com": {
          //     "NSTemporaryExceptionAllowsInsecureHTTPLoads": true
          //   }
          // }
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.wmaethner.PickEmLeague2023_Mobile"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "6d85651c-e34c-4518-a562-677646b70584"
      }
    }
  }
}
